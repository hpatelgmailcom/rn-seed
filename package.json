{
  "name": "PunchClock",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest --detectOpenHandles",
    "lint-fix": "eslint --fix ./App",
    "lint-check": "eslint ./App",
    "rename": "react-native-rename",
    "prepush": "detox test"
  },
  "dependencies": {
    "@sentry/react-native": "^1.0.4",
    "axios": "^0.19.0",
    "i18n-js": "^3.3.0",
    "material-bread": "^0.2.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.26",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-native": "0.59.9",
    "react-native-app-intro-slider": "^3.0.0",
    "react-native-config": "^0.11.7",
    "react-native-copilot": "^2.4.5",
    "react-native-device-info": "^2.3.2",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-push-notification": "^3.1.9",
    "react-native-share": "^2.0.0",
    "react-native-svg": "^9.8.4",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.6.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-form": "^8.2.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^1.0.2",
    "reduxsauce": "^1.0.1",
    "validate.js": "^0.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@react-native-community/eslint-config": "^0.0.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.5.0",
    "detox": "^14.1.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-native": "^3.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "metro-react-native-babel-preset": "^0.54.1",
    "react-native-rename": "^2.4.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native)/.+"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/AirClockApp/Build/Products/Debug-iphonesimulator/AirClockApp.app",
        "build": "xcodebuild -project ios/AirClockApp.xcodeproj -UseModernBuildSystem=NO -scheme AirClockApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    },
    "test-runner": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix ./App",
      "git add"
    ],
    "*.{css,json}": [
      "git add"
    ]
  }
}
